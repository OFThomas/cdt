   "source": [
    "print c+s"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "diagc=Matrix(n,n, lambda i,j: c if i==j else s if i+j==n-1 else 0)\n"
   ]
  },
